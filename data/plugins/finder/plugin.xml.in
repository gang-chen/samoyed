<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <id>finder</id>
  <_name>Finder</_name>
  <_description>This plugin adds search functionality.</_description>
  <module>finder</module>
  <extension>
    <id>find-text</id>
    <point>actions</point>
    <detail>
      <name>find-text</name>
      <path>/main-menu-bar/search</path>
      <path-2>/main-toolbar</path-2>
      <_label>Find _Text...</_label>
      <_tooltip>Find text in the current file</_tooltip>
      <icon-name>edit-find</icon-name>
      <accelerator>&lt;Control&gt;f</accelerator>
      <separate>1</separate>
    </detail>
  </extension>
  <extension>
    <id>histories</id>
    <point>histories</point>
    <detail/>
  </extension>
</plugin>
